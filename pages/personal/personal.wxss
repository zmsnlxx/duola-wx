.personal {
  min-height: 100vh;
  background: #EDEDED;
}
.personal .header {
  height: 70px;
  background: #161616;
}
.personal .user-card {
  width: calc(100% - 32px);
  margin: -56px auto 16px auto;
  height: 150px;
  background: linear-gradient(180deg, #EDEDED 0%, #FFFFFF 100%);
  border-radius: 8px;
  padding: 40px;
  box-sizing: border-box;
  display: flex;
}
.personal .box {
  width: calc(100% - 32px);
  margin: 0 auto;
}
.personal .box .van-cell {
  height: 52px;
  background: #FFFFFF;
  border-radius: 8px;
  margin-bottom: 2px;
}
.personal .box .van-cell .van-cell__value {
  font-size: 10px;
}
.personal .custom-tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
}
