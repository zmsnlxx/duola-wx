.home {
  height: 100vh;
  box-sizing: border-box;
}
.home image {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  right: 0;
  z-index: 1;
}
.home .content {
  width: 100%;
  height: 100%;
  z-index: 2;
  position: relative;
  color: #FFFFFF;
}
.home .content .title {
  margin: 0 0 17px 24px;
  font-size: 22px;
  font-weight: 600;
}
.home .content .msg {
  margin: 0 0 17px 24px;
  font-size: 14px;
}
.home .content .van-button {
  width: calc(100% - 48px);
  position: absolute;
  bottom: 30px;
  left: 24px;
}
